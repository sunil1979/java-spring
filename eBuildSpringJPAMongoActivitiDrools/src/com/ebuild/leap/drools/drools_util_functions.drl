function Boolean containsItem1(ListA, SetB){
		System.out.println("List A :" + ListA.toString());
		System.out.println("ListB :" + ListB.toString());
		for (Long item1 : ListA) {
			for (Theme theme : ListB) {
				System.out.println("Theme Id :"+theme.getId());
				if (theme.getId().equals(item1)) {
					System.out.println("contains");
					return true;
				}
			}
		}
